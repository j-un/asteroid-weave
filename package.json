{
  "name": "asteroid-weave",
  "version": "1.0.0",
  "description": "3D Space Endless Runner",
  "type": "module",
  "main": "dist/script.js",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "serve": "vite preview",
    "clean": "rm -rf dist",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint \"{src,test}/**/*.ts\""
  },
  "keywords": [
    "game",
    "three.js",
    "typescript",
    "3d"
  ],
  "author": "Jun Tanaka",
  "license": "MIT",
  "dependencies": {
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/three": "^0.180.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.2"
  }
}
